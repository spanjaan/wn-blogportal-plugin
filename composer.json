{
    "name": "spanjaan/wn-blogportal-plugin",
    "type": "winter-plugin",
    "version": "1.0.6",
    "description": "Extends Winter's Blog plugin with tag, archives, comments, and post share.",
    "keywords": [
        "winter", 
        "blogportal",
        "wintercms", 
        "blog",
        "archives"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sam",
            "email": "sam@rat.md",
            "role": "Original Author"
        },
        {
            "name": "S.p. Anjaan",
            "email": "spanjaan@gmail.com",
            "role": "Author"
        }
    ],
    "support": {
        "issues": "https://github.com/spanjaan/wn-blogportal-plugin/issues",
        "source": "https://github.com/spanjaan/wn-blogportal-plugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "composer/installers": "~1.0",
        "gregwar/captcha": "^1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "replace": {
        "ratmd/bloghub-plugin": "~1.3.0"
    }
}
